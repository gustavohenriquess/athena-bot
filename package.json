{
  "name": "athena-bot",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/gustavohenriquess/athena-bot.git",
  "author": "Gustavo Henrique <gustavo._henrique@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "doc": "yarn docsify serve docs",
    "start": "NODE_ENV=production node -r dotenv-flow/config dist/src/infra/http/server.js",
    "start:dev": "NODE_OPTIONS='--require \"tsconfig-paths/register\"' NODE_ENV=development tsnd -r dotenv-flow/config src/infra/http/server.ts",
    "watch": "tsc -w",
    "compile": "tsc",
    "run:dev": "NODE_ENV=development node --watch -r dotenv-flow/config src/server.js",
    "run:prd": "NODE_ENV=production node --watch -r dotenv-flow/config src/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.18",
    "docsify-cli": "^4.4.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/cron": "^2.0.0",
    "cron": "^2.2.0",
    "discord.js": "^14.7.1",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "openai": "^3.1.0"
  }
}
