{
  "compilerOptions": {
   "target": "es2016",
   "module": "commonjs",
   "rootDir": "./",
   "baseUrl": "./src",
   "paths": {
     "@modules/*": ["modules/*"],
     "@infra/*": ["infra/*"]
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  "forceConsistentCasingInFileNames": true
  }
}
